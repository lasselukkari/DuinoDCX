{
  "name": "DuinoDCX",
  "description": "Standalone WIFI remote controller for the Behringer Ultradrive Pro (DCX2496)",
  "version": "0.0.25",
  "author": "Lasse Lukkari <lasse.lukkari@gmai.com>",
  "contributors": [
    "Ilkka Huhtakallio"
  ],
  "repository": "github:lasselukkari/DuinoDCX",
  "license": "MIT",
  "devDependencies": {
    "awot-scripts": "0.0.31",
    "eslint-config-xo-react": "0.19.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-react": "7.14.2",
    "prettier": "1.18.2",
    "react-scripts": "3.0.1",
    "xo": "0.24.0"
  },
  "dependencies": {
    "bootswatch": "4.3.1",
    "compare-versions": "3.5.0",
    "lodash.clonedeep": "4.5.0",
    "lodash.isequal": "4.5.0",
    "mathjs": "6.0.2",
    "prop-types": "15.7.2",
    "rc-slider": "8.6.13",
    "rc-tooltip": "3.7.3",
    "react": "16.8.6",
    "react-block-ui": "1.1.4",
    "react-bootstrap": "1.0.0-beta.9",
    "react-bootstrap-dialog": "0.11.0",
    "react-breakpoints": "3.0.3",
    "react-dom": "16.8.6",
    "react-dropzone": "10.1.5",
    "react-icons": "3.7.0",
    "react-numeric-input": "2.2.3",
    "react-spinkit": "3.0.0",
    "react-toastify": "5.3.1",
    "react-window-size": "1.2.2",
    "recharts": "1.6.2",
    "superagent": "5.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "dist": "awot-static",
    "flash": "awot-command-line",
    "lint": "xo --fix",
    "upload": "react-scripts build && awot-static && awot-command-line"
  },
  "xo": {
    "extends": "xo-react",
    "globals": [
      "afterAll",
      "afterEach",
      "beforeAll",
      "beforeEach",
      "expect",
      "jest",
      "test",
      "describe",
      "it",
      "document",
      "fetch",
      "window",
      "FormData",
      "URLSearchParams",
      "atob",
      "btoa"
    ],
    "plugins": [
      "babel"
    ],
    "parser": "babel-eslint",
    "space": true,
    "prettier": true,
    "rules": {
      "react/jsx-pascal-case": "off",
      "unicorn/filename-case": "off",
      "react/forbid-component-props": "off"
    }
  },
  "awot-static": {
    "sources": "./build",
    "indexFile": "index.html",
    "sketchDir": "./DuinoDCX",
    "exclude": [
      "*.map",
      "service-worker.js"
    ]
  },
  "awot-command-line": {
    "sketch": "DuinoDCX/DuinoDCX.ino",
    "idePath": "/Applications/Arduino.app/Contents/MacOS/arduino",
    "port": "/dev/cu.Repleo-PL2303-00002014",
    "board": {
      "package": "esp32",
      "arch": "esp32",
      "board": "esp32"
    },
    "action": "upload"
  },
  "proxy": "http://192.168.1.110",
  "renovate": {
    "extends": [
      "config:base"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
